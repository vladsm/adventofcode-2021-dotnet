	</PropertyGroup>

	<PropertyGroup>
		<UserSecretsId>88bd5ee1-ffc2-4bf2-acd0-4fe6ec2da75b</UserSecretsId>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="AdventOfCode.Runner" Version="1.0.0" />
